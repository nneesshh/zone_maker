    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'zone_maker'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=zone_maker",
                    "--package=zone_maker"
                ],
                "filter": {
                    "name": "zone_maker",
                    "kind": "bin"
                }
            },
            //"args": ["ini", "-i", "res/zone.ini", "templates/default.tpl"],
            //"args": ["db", "-h", "127.0.0.1", "-u", "root", "-p", "123456", "-D", "test_gpaas", "templates/default.tpl"],
            "args": ["excel", "-i", "res/文竹海外-配置源.xlsx", "templates/default.tpl"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'zone_maker'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=zone_maker",
                    "--package=zone_maker"
                ],
                "filter": {
                    "name": "zone_maker",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}